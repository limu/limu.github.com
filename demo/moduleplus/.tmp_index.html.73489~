<!DOCTYPE HTML>
<html>
    <head>
        <meta charset="utf-8">
        <title>Hello SeaJS!</title>
    </head>
    <body>
        <script type="text/javascript" src="libs/seajs/sea-debug.js">
        </script>
        <script type="text/javascript">
            (function(){
                var currentPagePath = location.href.split("#")[0].split("index.html")[0];
                seajs.config({
                    debug: 2,
                    alias: {
                        libs: currentPagePath + "libs",
                        modplus: currentPagePath + "modplus",
                        mptest: currentPagePath + "mptest"
                    }
                });
                seajs.use(["modplus/mp", "./main"]);
            })();
        </script>
    </body>
</html>
