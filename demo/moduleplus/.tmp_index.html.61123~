        <script type="text/javascript">
            (function(){
                var currentPagePath = location.href.split("#")[0].split("index.html")[0];
                seajs.config({
                    debug: 2,
                    alias: {
                        test: currentPagePath + "test",
                        libs: currentPagePath + "libs"
                    }
                });
                seajs.use(["libs/seajs/moduleplus", "./main"]);
            })();
        </script>
